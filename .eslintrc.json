{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "camelcase": 0,
    "quotes": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "implicit-arrow-linebreak": 0
  },
  "ignorePatterns": ["**/node_modules/**", "**/dist/**", "**/lib/**"]
}
